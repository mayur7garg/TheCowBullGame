{
	"name": "Cow Bull Game Webapp",
	"dockerComposeFile": [
		"../docker-compose.yml"
	],
	"service": "webapp",
	"forwardPorts": [80],
	"customizations": {
		"vscode": {
			"settings": {
				"python.defaultInterpreterPath": "/usr/local/bin/python"
			},
			"extensions": [
				"ms-python.python"
			]
		}
	},
	"workspaceFolder": "/code"
}